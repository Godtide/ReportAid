  date = {1975-11-18},
  pages = {5168--5175},
  keywords = {Binding Sites,Binding; Competitive,Dinitrofluorobenzene,Hydrogen-Ion Concentration,Kinetics,Histidine,Muramidase,Peptide Hydrolases,Protein Binding,Streptomyces griseus,Trypsin,Valine},
