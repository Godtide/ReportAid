{
  "name": "ReportAid",
  "version": "0.0.5",
  "description": "Humanitarian Aid Reporting",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glowkeeper/HumanitarianBlockchain.git"
  },
  "scripts": {
    "generate": "cd ./src/blockchain && mkdir -p ./build/contracts && touch ./build/contracts/fooBar.json && rm ./build/contracts/*json && truffle compile --all && ./typechainBuild.sh",
    "develMigrate": "cd ./src/blockchain && truffle migrate --network development",
    "rinkebyMigrate": "cd ./src/blockchain && truffle migrate --network rinkeby",
    "ropstenMigrate": "cd ./src/blockchain && truffle migrate --network ropsten",
    "compile": "./node_modules/.bin/webpack --config ./webpack.dev.js --mode development",
    "watch": "./node_modules/.bin/webpack --config ./webpack.dev.js --watch --mode development",
    "dev": "./node_modules/.bin/webpack --config ./webpack.dev.js --mode development",
    "prod": "./node_modules/.bin/webpack --config ./webpack.prod.js --mode production",
    "test": "./node_modules/.bin/mocha",
    "start": "http-server -p 8081 build/"
  },
  "author": "Steve Huckle",
